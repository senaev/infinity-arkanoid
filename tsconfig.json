{
  "compilerOptions": {
    "module": "commonjs",
    "target": "es6",
    "noImplicitAny": true,
    "noFallthroughCasesInSwitch": true,
    "strictNullChecks": true,
    "noImplicitThis": true,
    "noUnusedParameters": false,
    "noUnusedLocals": true,
    "noImplicitReturns": true,
    "pretty": true,
    "moduleResolution": "node"
  },
  "exclude": [
    "node_modules"
  ]
}
